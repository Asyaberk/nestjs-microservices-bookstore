{
  "name": "nestjs-user-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:dev": "concurrently -c \"cyan.bold,green.bold,blue.bold,magenta.bold,yellow.bold,white.bold\" \"npm:dev-*\"",
    "start": "nest start",
    "dev-gateway": "nest start --watch api-gateway",
    "dev-books": "nest start --watch books",
    "dev-library": "nest start --watch library",
    "dev-roles": "nest start --watch roles",
    "dev-users": "nest start --watch users",
    "dev-auth": "nest start --watch auth",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/nestjs-user-project/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch --maxWorkers=1",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch"
  },
  "dependencies": {
    "@keyv/redis": "^5.1.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "async-retry": "^1.3.3",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.1.1",
    "cacheable": "^1.10.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "kafkajs": "^2.2.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "redis": "^4.7.1",
    "redis-cli": "^2.1.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/cache-manager": "^4.0.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.17.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "redis-commander": "^0.9.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@app/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/decorators(|/.*)$": "<rootDir>/libs/decorators/src/$1",
      "^@app/dtos(|/.*)$": "<rootDir>/libs/dtos/src/$1",
      "^@app/entities(|/.*)$": "<rootDir>/libs/entities/src/$1",
      "^@app/guards(|/.*)$": "<rootDir>/libs/guards/src/$1",
      "^@app/kafka(|/.*)$": "<rootDir>/libs/kafka/src/$1",
      "^@app/roles(|/.*)$": "<rootDir>/libs/roles/src/$1",
      "^@app/strategy(|/.*)$": "<rootDir>/libs/strategy/src/$1"
    }
  }
}
