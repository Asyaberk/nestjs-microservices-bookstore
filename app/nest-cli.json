{
  "$schema": "http://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "app/api-gateway/src",
  "monorepo": true,
  "root": "app/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "app/api-gateway",
      "entryFile": "main",
      "sourceRoot": "app/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "app/api-gateway/tsconfig.app.json"
      }
    },
    "books": {
      "type": "application",
      "root": "app/books",
      "entryFile": "main",
      "sourceRoot": "app/books/src",
      "compilerOptions": {
        "tsConfigPath": "app/books/tsconfig.app.json"
      }
    },
    "library": {
      "type": "application",
      "root": "app/library",
      "entryFile": "main",
      "sourceRoot": "app/library/src",
      "compilerOptions": {
        "tsConfigPath": "app/library/tsconfig.app.json"
      }
    },
    "roles": {
      "type": "application",
      "root": "app/roles",
      "entryFile": "main",
      "sourceRoot": "app/roles/src",
      "compilerOptions": {
        "tsConfigPath": "app/roles/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "app/users",
      "entryFile": "main",
      "sourceRoot": "app/users/src",
      "compilerOptions": {
        "tsConfigPath": "app/users/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "app/auth",
      "entryFile": "index",
      "sourceRoot": "app/auth/src",
      "compilerOptions": {
        "tsConfigPath": "app/auth/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "dtos": {
      "type": "library",
      "root": "libs/dtos",
      "entryFile": "index",
      "sourceRoot": "libs/dtos/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dtos/tsconfig.lib.json"
      }
    },
    "entities": {
      "type": "library",
      "root": "libs/entities",
      "entryFile": "index",
      "sourceRoot": "libs/entities/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entities/tsconfig.lib.json"
      }
    },
    "guards": {
      "type": "library",
      "root": "libs/guards",
      "entryFile": "index",
      "sourceRoot": "libs/guards/src",
      "compilerOptions": {
        "tsConfigPath": "libs/guards/tsconfig.lib.json"
      }
    },
    "kafka": {
      "type": "library",
      "root": "libs/kafka",
      "entryFile": "index",
      "sourceRoot": "libs/kafka/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka/tsconfig.lib.json"
      }
    },
    "strategy": {
      "type": "library",
      "root": "libs/strategy",
      "entryFile": "index",
      "sourceRoot": "libs/strategy/src",
      "compilerOptions": {
        "tsConfigPath": "libs/strategy/tsconfig.lib.json"
      }
    },
    "decorators": {
      "type": "library",
      "root": "libs/decorators",
      "entryFile": "index",
      "sourceRoot": "libs/decorators/src",
      "compilerOptions": {
        "tsConfigPath": "libs/decorators/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  }
}
